########################################################################
#	hqos: 1:ON, 0:OFF                                              #
#	txq_num: 16                                                    #
#	schX_enable: 1:ON, 0:OFF                                       #
#	schX_mode: schX mode                                           #
#	0:weighted round-robin 1:strict priority                       #
#	schX_bw: schX bandwidth (unit:Kbps)                            #
########################################################################
config 'global'
	option hqos 0
	option txq_num 16
	option sch0_enable 0
	option sch0_mode 0
	option sch0_bw 1000000
	option sch1_enable 0
	option sch1_mode 0
	option sch1_bw 1000000
	option sch2_enable 0
	option sch2_mode 0
	option sch2_bw 1000000
	option sch3_enable 0
	option sch3_mode 0
	option sch3_bw 1000000

####################################################################
#	id: queue id (0-15)                                        #
#	sch: sch id (0-3)                                          #
#	minrate: percentage of min rate limit                      #
#	maxrate: percentage of max rate limit                      #
#	weight: weight for queue schedule                          #
#	resv: buffer reserved for HW/SW path                       #
####################################################################
config queue
        option id 0
        option sch 0
        option minrate 30
        option maxrate 100
        option weight 4
        option resv 4

